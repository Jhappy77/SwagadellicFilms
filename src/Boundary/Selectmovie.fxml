      <Label layoutX="14.0" layoutY="116.0" text="Browse Showtimes" />
      <Button layoutX="50.0" layoutY="196.0" mnemonicParsing="false" onAction="#goBack" style="-fx-background-color: #cdcdcd;" text="Go Back">
         <font>
            <Font size="14.0" />
         </font></Button>
      <ComboBox fx:id="cbbm" layoutX="119.0" layoutY="46.0" onAction="#chooseTheatre" prefWidth="150.0" />
      <ComboBox fx:id="cbbt" layoutX="119.0" layoutY="77.0" onAction="#chooseScreening" prefWidth="150.0" />
      <ComboBox fx:id="cbbs" layoutX="119.0" layoutY="113.0" onAction="#Continue" prefWidth="150.0" />
      <Label fx:id="status" layoutX="14.0" layoutY="154.0" prefHeight="17.0" prefWidth="189.0" text="Status" textFill="RED">
         <font>
            <Font size="14.0" />
         </font></Label>
      <Button layoutX="177.0" layoutY="196.0" mnemonicParsing="false" onAction="#selectSeat" style="-fx-background-color: #ff4c4c;" text="Select seats" textFill="WHITE">
